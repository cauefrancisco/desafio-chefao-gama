#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Kelbass/Dev/GamaAcademy/DesafioChefão/desafio-chefao-gama/packages/server/node_modules/.pnpm/mime@1.6.0/node_modules/mime/node_modules:/mnt/d/Kelbass/Dev/GamaAcademy/DesafioChefão/desafio-chefao-gama/packages/server/node_modules/.pnpm/mime@1.6.0/node_modules:/mnt/d/Kelbass/Dev/GamaAcademy/DesafioChefão/desafio-chefao-gama/packages/server/node_modules/.pnpm/node_modules:/mnt/d/Kelbass/Dev/GamaAcademy/DesafioChefão/desafio-chefao-gama/packages/server/node_modules:/mnt/d/Kelbass/Dev/GamaAcademy/DesafioChefão/desafio-chefao-gama/packages/node_modules:/mnt/d/Kelbass/Dev/GamaAcademy/DesafioChefão/desafio-chefao-gama/node_modules:/mnt/d/Kelbass/Dev/GamaAcademy/DesafioChefão/node_modules:/mnt/d/Kelbass/Dev/GamaAcademy/node_modules:/mnt/d/Kelbass/Dev/node_modules:/mnt/d/Kelbass/node_modules:/mnt/d/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/Kelbass/Dev/GamaAcademy/DesafioChefão/desafio-chefao-gama/packages/server/node_modules/.pnpm/mime@1.6.0/node_modules/mime/node_modules:/mnt/d/Kelbass/Dev/GamaAcademy/DesafioChefão/desafio-chefao-gama/packages/server/node_modules/.pnpm/mime@1.6.0/node_modules:/mnt/d/Kelbass/Dev/GamaAcademy/DesafioChefão/desafio-chefao-gama/packages/server/node_modules/.pnpm/node_modules:/mnt/d/Kelbass/Dev/GamaAcademy/DesafioChefão/desafio-chefao-gama/packages/server/node_modules:/mnt/d/Kelbass/Dev/GamaAcademy/DesafioChefão/desafio-chefao-gama/packages/node_modules:/mnt/d/Kelbass/Dev/GamaAcademy/DesafioChefão/desafio-chefao-gama/node_modules:/mnt/d/Kelbass/Dev/GamaAcademy/DesafioChefão/node_modules:/mnt/d/Kelbass/Dev/GamaAcademy/node_modules:/mnt/d/Kelbass/Dev/node_modules:/mnt/d/Kelbass/node_modules:/mnt/d/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
